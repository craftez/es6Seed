{
  "name": "angular-webpack",
  "version": "1.0.0",
  "description": "Angular ES6 Starter",
  "scripts": {
    "clean": "rimraf dist",
    "server": "webpack-dev-server --history-api-fallback --inline --progress --color",
    "start": "npm run server",
    "build": "npm run clean && webpack --bail --progress --profile --color -p",
    "watch": "npm run clean && webpack --bail --progress --profile --color -w",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run"
  },
  "author": "Eduardo Zamora",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-config-google": "^0.6.0",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.7.1",
    "isparta-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.14",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^4.0.0",
    "sasslint-loader": "0.0.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
